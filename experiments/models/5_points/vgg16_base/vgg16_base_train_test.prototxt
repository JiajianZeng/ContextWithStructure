layer {
  name: "performance"
  type: "FacialLandmarkPerformance"
  bottom: "fc8"
  bottom: "landmark"
  bottom: "eyedist"
  top: "average_error"
  top: "false_rate"
  
  facial_landmark_performance_param {
    num_landmark: 5
    error_threshold: 0.1
  }
  include {
    phase: TEST
  }
}

layer {
  name: "euclidean_loss"
  type: "EuclideanLoss"
  bottom: "fc8"
  bottom: "landmark"
  top: "euclidean_loss"
}
